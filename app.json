      "eas": {
        "projectId": "1b3a84d4-e918-4ec7-8ee1-da07958795e2"
      }
    },
    "plugins": [
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-8988048891909123~1615946640"
        }
      ],
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
