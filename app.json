    "name": "Dentra",
    "slug": "dentra",
    "scheme": "com.dentra.dentraapp",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon3.png",
    "userInterfaceStyle": "automatic",
